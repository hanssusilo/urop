#ifndef OBSTACLES_H
#define OBSTACLES_H

#include <sstream>
class Obstacles {
  public:
    double lx;
    double ly;
    double ux;
    double uy;
  
    std::string print() {
        stringstream sstm;
        sstm << this->lx << ',' << this->ux << ',' << this->ly << ',' << this->uy << endl;
        return sstm.str();
}
};
#endif
